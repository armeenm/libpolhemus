get_filename_component(Polhemus_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
include(CMakeFindDependencyMacro)

list(APPEND CMAKE_MODULE_PATH ${Polhemus_CMAKE_DIR})
find_dependency(LibUSB 1.0 REQUIRED MODULE)
list(REMOVE_AT CMAKE_MODULE_PATH -1)

if(NOT TARGET Polhemus::Polhemus OR NOT TARGET Polhemus::CPolhemus)
    include("${Polhemus_CMAKE_DIR}/PolhemusTargets.cmake")
endif()

set(Polhemus_LIBRARIES "Polhemus::Polhemus;Polhemus::CPolhemus")
